<% content_for(:title) { 'Who We Are' } %>
<div class="pagetitle"><h2>RubyGardens faces</h2></div>

<table class="table">
<% @emails.in_groups_of(4, false) do |row_emails| %>
  <tr>
    <% for email in row_emails %>
      <td><%= image_tag gravatar_url email %></td>
    <% end %>
  </tr>
<% end %>
</table>